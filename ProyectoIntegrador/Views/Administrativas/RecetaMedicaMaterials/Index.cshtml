@model IEnumerable<ProyectoIntegrador.Models.DBModels.RecetaMedicaMaterial>

@{
    ViewBag.Title = "Index";
}

<h2>Receta Médica - Materiales</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaterialF.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RecetaMedicaF.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaterialF.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RecetaMedicaF.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cantidad)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
